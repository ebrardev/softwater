{
    "manifest_version": 3,
    "name": "Softwater",
    "version": "1.0",
    "description": "Daily water tracker",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png"
      },
      "default_popup": "popup.html"
    }
  }
  